<ehcache name="cacheManagerOne" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">

    <terracottaConfig url="localhost:9510"/>

    <cache name="Cache1"
           maxEntriesLocalHeap="10000"
           maxEntriesInCache="50000"
           eternal="false"
           timeToIdleSeconds="0"
           timeToLiveSeconds="300" statistics="true">
        <terracotta concurrency="128" consistency="strong"/>
    </cache>
    <cache name="Cache2"
           maxEntriesLocalHeap="10000"
           maxEntriesInCache="50000"
           eternal="false"
           timeToIdleSeconds="200"
           timeToLiveSeconds="0" statistics="true">
        <terracotta concurrency="128" consistency="strong"/>
    </cache>

</ehcache>